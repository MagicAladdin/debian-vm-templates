{
  "description": "Contrib Debian Jessie base box, build using contrib sources",
  "variables": {
      "box_version": "8.5.0",
      "box_changelog": "* new stable point release",
      "atlas_token": "{{env `ATLAS_TOKEN`}}"
    },
  "builders": [
    {
      "boot_command": [
        "<esc><wait>",
        "install <wait>",
        "preseed/url=http://{{ .HTTPIP }}:{{ .HTTPPort }}/vanilla-debian-8-jessie-preseed.cfg <wait>",
        "debian-installer=en_US <wait>",
        "auto <wait>",
        "locale=en_US <wait>",
        "kbd-chooser/method=us <wait>",
        "netcfg/get_hostname={{ .Name }} <wait>",
        "netcfg/get_domain=vagrantup.com <wait>",
        "fb=false <wait>",
        "debconf/frontend=noninteractive <wait>",
        "console-setup/ask_detect=false <wait>",
        "console-keymaps-at/keymap=us <wait>",
        "keyboard-configuration/xkb-keymap=us <wait>",
        "<enter><wait>"
      ],
      "disk_size": "10140",
      "disk_interface": "virtio",
      "headless": false,
      "http_directory": "http",
      "iso_checksum": "ad4e8c27c561ad8248d5ebc1d36eb172f884057bfeb2c22ead823f59fa8c3dff",
      "iso_checksum_type": "sha256",
      "iso_url": "http://cdimage.debian.org/cdimage/release/8.5.0/amd64/iso-cd/debian-8.5.0-amd64-netinst.iso",
      "shutdown_command": "sudo /sbin/halt -p",
      "ssh_password": "vagrant",
      "ssh_username": "vagrant",
      "ssh_port": "22",
      "ssh_wait_timeout": "10000s",
      "type": "qemu",
      "qemuargs": [[ "-m", "1024M" ],[ "-smp", "2" ]],
      "accelerator": "kvm",
      "vm_name": "contrib-jessie.raw",
      "output_directory": "contrib-jessie.build",
      "format": "raw"
    }
  ],
  "provisioners": [
    {
      "type": "shell",
      "pause_before": "5s",
      "execute_command": "echo 'vagrant'|sudo -S sh '{{.Path}}'",
      "override": {
        "qemu": {
          "scripts": [
            "scripts/base.sh",
            "scripts/vagrant.sh",
            "scripts/vb-guest-additions.sh",
            "scripts/cleanup.sh",
            "scripts/minimize.sh"
          ]
        }
      }
    }
  ],
  "push": {
    "vcs": true,
    "name": "debian/contrib-jessie64"
  }
}

